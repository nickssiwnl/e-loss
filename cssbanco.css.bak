body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            background-color: #ffffff;
			background-size: cover;
        }
		.head {
			
		color :white;	
			
		}
        .header {
            background: #02c1e3;
            color: white ;
            padding: 10px 0;
            text-align: left;
            padding-left: 20px;
        }
		.title
		{
			color :white;
			
		}
        .header .username {
            font-weight: bold;
        }
        .container {
            width: 80%;
            margin: auto;
            overflow: hidden;
            margin-top: 20px;
			 color: white;
        }
        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
			 color: white;
        }
        table, th, td {
            border: 1px solid #ddd;
			 color: white;
        }
        th, td {
            padding: 8px;
            text-align: left; 
			color: white;
			
        }
        th {
            background-color: #02c1e3 ;
            color: white;
        }
        tr:nth-child(even) {
            background-color: #02c1e3 ;
        }
        .buttons {
            margin-top: 20px;
			border-radius:30px;
        }
        .buttons button {
            padding: 10px 15px;
            margin-right: 10px;
            border: none;
            background-color: #02c1e3 ;
            color: white;
            cursor: pointer;
        }
        .buttons button:hover {
            background-color: #02c1e3 ;
        }